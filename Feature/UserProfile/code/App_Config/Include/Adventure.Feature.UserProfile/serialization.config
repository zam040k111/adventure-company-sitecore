<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <unicorn>
      <configurations>
        <configuration name="Feature.UserProfile" extends="Helix.Base" dependencies="Foundation.*">
          <predicate>
            <include name="Templates" database="master" path="/sitecore/templates/Feature/UserProfile" />
            <include name="Renderings" database="master" path="/sitecore/layout/Renderings/Feature/UserProfile" />
            <include name="Dictionary" database="master" path="/sitecore/system/Dictionary/Feature/UserProfile" />
            <include name="Content" database="master" path="/sitecore/content/Adventure/Home/AccountHeader/UserProfile" />
            <include name="Media library" database="master" path="/sitecore/media library/Feature/UserProfile" />
          </predicate>
        </configuration>

      </configurations>
    </unicorn>
  </sitecore>
</configuration>
